{
    "workbench.colorCustomizations": {
        "activityBar.activeBackground": "#c160a6",
        "activityBar.activeBorder": "#b2c975",
        "activityBar.background": "#c160a6",
        "activityBar.foreground": "#15202b",
        "activityBar.inactiveForeground": "#15202b99",
        "activityBarBadge.background": "#b2c975",
        "activityBarBadge.foreground": "#15202b"
    },
    "peacock.color": "#ab438e",
    "git.ignoreLimitWarning": true,
    "cSpell.words": [
        "Gtree",
        "Posix",
        "cmds",
        "elems",
        "expl",
        "glayer",
        "interp"
    ],
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/.classpath": true,
        "**/.factorypath": true,
        "**/.project": true,
        "**/.settings": true,
        "**/*.meta": true,
        "emg_bind": false
    },
    "rust-analyzer.checkOnSave.allTargets": false,
    "rust-analyzer.cargo.target": "wasm32-unknown-unknown",
}